{
  "name": "LowCode",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:all": "concurrently \"pnpm dev\" \"pnpm dev:account\" \"pnpm dev:admin\" \"pnpm dev:renderer\" \"pnpm dev:designer\"",
    "dev": "vite --mode development",
    "dev:account": "pnpm --filter LowCode/account dev",
    "dev:admin": "pnpm --filter LowCode/admin dev",
    "dev:renderer": "pnpm --filter LowCode/renderer dev",
    "dev:designer": "pnpm --filter LowCode/designer dev",
    "dev:test": "vite --mode test",
    "dev:production": "vite --mode production",
    "build:development": "vue-tsc -b && vite build --mode development",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:production": "vue-tsc -b && vite build --mode production",
    "build:account": "pnpm --filter LowCode/account build:production",
    "build:admin": "pnpm --filter LowCode/admin build:production",
    "build:renderer": "pnpm --filter LowCode/renderer build:production",
    "build:designer": "pnpm --filter LowCode/designer build:production",
    "build:all": "pnpm build:production && pnpm build:account && pnpm build:admin && pnpm build:renderer && pnpm build:designer",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\""
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-qiankun": "^1.0.15",
    "vue-tsc": "^3.2.1"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "element-plus": "^2.13.0",
    "pinia": "^2.3.0",
    "qiankun": "^2.10.16",
    "vue": "^3.5.26",
    "vue-router": "^4.5.0"
  }
}
