{
  "name": "LowCode",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "--------------------------------开发环境--------------------------------": "--------------------------------开发环境--------------------------------",
    "dev:all-development": "concurrently \"pnpm dev\" \"pnpm dev:account\" \"pnpm dev:admin\" \"pnpm dev:renderer\" \"pnpm dev:designer\"",
    "build:all-development": "pnpm build:development && pnpm build:account:development && pnpm build:admin:development && pnpm build:renderer:development && pnpm build:designer:development",
    "dev": "vite --mode development",
    "dev:account": "pnpm --filter LowCode/account dev",
    "dev:admin": "pnpm --filter LowCode/admin dev",
    "dev:renderer": "pnpm --filter LowCode/renderer dev",
    "dev:designer": "pnpm --filter LowCode/designer dev",
    "build:development": "vue-tsc -b && vite build --mode development",
    "build:account:development": "pnpm --filter LowCode/account build:development",
    "build:admin:development": "pnpm --filter LowCode/admin build:development",
    "build:renderer:development": "pnpm --filter LowCode/renderer build:development",
    "build:designer:development": "pnpm --filter LowCode/designer build:development",
    "--------------------------------测试环境--------------------------------": "--------------------------------测试环境--------------------------------",
    "dev:all-test": "concurrently \"pnpm dev:test\" \"pnpm dev:account:test\" \"pnpm dev:admin:test\" \"pnpm dev:renderer:test\" \"pnpm dev:designer:test\"",
    "build:all-test": "pnpm build:test && pnpm build:account:test && pnpm build:admin:test && pnpm build:renderer:test && pnpm build:designer:test",
    "dev:test": "vite --mode test",
    "dev:account:test": "pnpm --filter LowCode/account dev:test",
    "dev:admin:test": "pnpm --filter LowCode/admin dev:test",
    "dev:renderer:test": "pnpm --filter LowCode/renderer dev:test",
    "dev:designer:test": "pnpm --filter LowCode/designer dev:test",
    "build:test": "vue-tsc -b && vite build --mode test",
    "build:account:test": "pnpm --filter LowCode/account build:test",
    "build:admin:test": "pnpm --filter LowCode/admin build:test",
    "build:renderer:test": "pnpm --filter LowCode/renderer build:test",
    "build:designer:test": "pnpm --filter LowCode/designer build:test",
    "--------------------------------生产环境--------------------------------": "--------------------------------生产环境--------------------------------",
    "dev:all-production": "concurrently \"pnpm dev:production\" \"pnpm dev:account:production\" \"pnpm dev:admin:production\" \"pnpm dev:renderer:production\" \"pnpm dev:designer:production\"",
    "build:all-production": "pnpm build:production && pnpm build:account:production && pnpm build:admin:production && pnpm build:renderer:production && pnpm build:designer:production",
    "dev:production": "vite --mode production",
    "dev:account:production": "pnpm --filter LowCode/account dev:production",
    "dev:admin:production": "pnpm --filter LowCode/admin dev:production",
    "dev:renderer:production": "pnpm --filter LowCode/renderer dev:production",
    "dev:designer:production": "pnpm --filter LowCode/designer dev:production",
    "build:production": "vue-tsc -b && vite build --mode production",
    "build:account:production": "pnpm --filter LowCode/account build:production",
    "build:admin:production": "pnpm --filter LowCode/admin build:production",
    "build:renderer:production": "pnpm --filter LowCode/renderer build:production",
    "build:designer:production": "pnpm --filter LowCode/designer build:production",
    "--------------------------------代码质量--------------------------------": "--------------------------------代码质量--------------------------------",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\""
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-qiankun": "^1.0.15",
    "vue-tsc": "^3.2.1"
  },
  "dependencies": {
    "@cwj/ui": "workspace:*",
    "consola": "^3.4.2",
    "element-plus": "^2.13.0",
    "interactjs": "^1.10.27",
    "pinia": "^2.3.0",
    "qiankun": "^2.10.16",
    "vue": "^3.5.26",
    "vue-router": "^4.5.0"
  }
}
